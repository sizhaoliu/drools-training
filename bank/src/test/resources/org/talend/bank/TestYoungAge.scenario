    <fact-data>
      <type>LoanSimulator</type>
      <fieldData>
        <field-data>
          <name>age</name>
          <value>16</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>rejected</name>
          <value>false</value>
          <nature>1</nature>
